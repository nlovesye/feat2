{
  "name": "node-server",
  "version": "0.0.1",
  "description": "nodejs服务",
  "main": "index.ts",
  "scripts": {
    "start": "nodemon --exec babel-node src/index.ts",
    "webpack:debug": "node --inspect-brk ./node_modules/.bin/webpack --inline --progress",
    "build": "cross-env NODE_ENV=prod webpack --config config/webpack.config.prod.ts",
    "watch": "cross-env NODE_ENV=dev webpack --watch --progress --config config/webpack.config.dev.ts",
    "debug": "nodemon --inspect=7200 ./dist/server.bundle.js",
    "dev": "npm-run-all -p watch debug",
    "clean": "rimraf dist",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nlovesye/node-server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nlovesye/node-server/issues"
  },
  "homepage": "https://github.com/nlovesye/node-server#readme",
  "dependencies": {
    "@koa/cors": "3.3.0",
    "bcrypt": "5.0.1",
    "bluebird": "3.7.2",
    "jsonwebtoken": "8.5.1",
    "koa": "2.13.4",
    "koa-body": "5.0.0",
    "koa-combine-routers": "4.0.2",
    "koa-compress": "5.1.0",
    "koa-helmet": "6.1.0",
    "koa-json": "2.0.2",
    "koa-jwt": "4.0.3",
    "koa-logger": "3.2.1",
    "koa-router": "11.0.1",
    "koa-static": "5.0.0",
    "moment": "2.29.4",
    "mongoose": "6.6.2",
    "nodemailer": "6.7.7",
    "redis": "3.1.2",
    "svg-captcha": "1.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@babel/node": "7.18.6",
    "@babel/preset-env": "7.18.6",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@types/koa": "2.13.5",
    "@types/koa__cors": "3.3.0",
    "@types/koa-compress": "4.0.3",
    "@types/koa-json": "2.0.20",
    "@types/koa-logger": "3.1.2",
