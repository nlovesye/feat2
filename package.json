    "nodemon": "2.0.19",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "terser-webpack-plugin": "5.3.3",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.4",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
